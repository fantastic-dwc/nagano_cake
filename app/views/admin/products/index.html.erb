<div class='container px-5 px-sm-0'>
  <div class="row">
      <h1>商品一覧</h1>
      <table class="table">
        <thead>
          <tr>
            <th>商品ID</th>
            <th>商品名</th>
            <th>税抜価格</th>
            <th>ジャンル</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @products.each do |f| %>
          <tr>
            <td><%= f.id %></td>
            <td><%= link_to f.name, admin_product_path(f) %></td>
            <td><%= f.price %></td>
            <td></td>
            <td></td>
          </tr>
          <% end %>
        </tbody>
      </table>

      <div>pagenation</div>

  </div>
</div>